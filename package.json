{
  "private": true,
  "lint-staged": {
    "**/*.php": [
      "npm run php-lint"
    ],
    "**/*.{ts,js,tsx}": [
      "node_modules/.bin/prettier --config .prettierrc --write *.{js,tsx,ts}"
    ],
    "**/*.{css,scss}": "node_modules/.bin/stylelint --ignore-path .stylelintignore 'resources/**/*.{scss,ts,tsx}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "dev": "vite",
    "build-staging": "vite build --mode staging",
    "build": "vite build",
    "eslint": "eslint --ext \".ts,.js,.tsx\" .",
    "eslint-fix": "eslint --fix --ext \".ts,.js\" .",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "php-lint": "sh php-lint.sh",
    "php-format": "./vendor/bin/php-cs-fixer --config=./.php-cs-fixer.dist.php fix -v --diff",
    "stylelint": "stylelint \"**/*.{vue,scss}\" --ignore-path .gitignore",
    "stylelint-fix": "stylelint '**/*.{scss,vue}' --fix",
    "test": "jest --config ./jest.config.json",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.5",
    "@types/js-cookie": "^3.0.2",
    "chart.js": "^3.9.1",
    "clsx": "^1.2.1",
    "faker": "5.5.3",
    "file-saver": "^2.0.5",
    "install": "^0.13.0",
    "js-cookie": "^3.0.1",
    "lint-staged": "^13.0.3",
    "moment": "^2.29.4",
    "object-to-formdata": "^4.4.2",
    "react-chartjs-2": "^4.3.1",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.35.0",
    "react-linkify": "^1.0.0-alpha",
    "react-newline-to-break": "^1.0.6",
    "react-redux": "^8.0.2",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-router-redux": "^4.0.8",
    "react-slick": "^0.29.0",
    "redux-persist": "^6.0.0",
    "sass": "^1.54.5",
    "sass-loader": "^13.0.2",
    "simplebar-react": "^2.4.3",
    "slick-carousel": "^1.8.1",
    "typescript": "^5.1.3",
    "ua-parser-js": "^1.0.2",
    "use-interval": "^1.4.0",
    "uuid": "^9.0.1",
    "vite-plugin-ssr": "^0.4.29"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/faker": "5.5.3",
    "@types/file-saver": "^2.0.5",
    "@types/jest": "^29.2.2",
    "@types/lodash": "^4.14.185",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-helmet": "^6.1.5",
    "@types/react-linkify": "^1.0.1",
    "@types/react-redux": "^7.1.24",
    "@types/react-slick": "^0.23.10",
    "@types/ua-parser-js": "^0.7.36",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.35.1",
    "@vitejs/plugin-react": "^2.0.1",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "axios": "^0.27",
    "eslint": "8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.1",
    "i": "^0.3.7",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "laravel-vite-plugin": "^0.5.0",
    "lodash": "^4.17.21",
    "npm": "^8.19.1",
    "postcss": "^8.1.14",
    "prettier": "^2.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recess-order": "^2.4.0",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-scss": "^3.19.0",
    "ts-jest": "^29.0.3",
    "vite": "^3.0.0"
  }
}
